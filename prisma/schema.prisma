datasource db {
    provider = "postgresql"
    url      = env("prisma_db_url")
}

generator client {
    provider = "prisma-client-js"
}

model Guild {
    guildSid String @id

    permissions   Permissions @relation(fields: [permissionsId], references: [id], onDelete: Cascade)
    permissionsId Int         @unique
}

model Permissions {
    id       Int     @id @default(autoincrement())
    guild    Guild?  @relation
    guildSid String? @unique

    adminSid String? @unique
}
