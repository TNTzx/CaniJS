datasource db {
    provider = "postgresql"
    url      = env("prisma_db_url")
}

generator client {
    provider = "prisma-client-js"
}

model Guild {
    guildSid    String       @id
    permissions Permissions?
}

model Permissions {
    guildSid String @id
    guild    Guild  @relation(fields: [guildSid], references: [guildSid])

    adminSid String? @unique
}
